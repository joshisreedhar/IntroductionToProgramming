import turtle

s = turtle.getscreen()
t = turtle.Turtle()
t.pen(pencolor="purple", fillcolor="red", pensize=1, speed=9)
t.begin_fill()
t.forward(120)
t.left(120)
t.forward(120)
t.left(120)
t.forward(120)
t.end_fill()

t.pen(pencolor="purple", fillcolor="blue", pensize=1, speed=9)
t.begin_fill()
t.right(120)
t.forward(120)
t.right(120)
t.forward(120)
t.end_fill
t.end_fill()

t.pen(pencolor="purple", fillcolor="green", pensize=1, speed=9)
t.begin_fill()
t.left(120)
t.forward(120)
t.left(120)
t.forward(120)
t.end_fill
t.end_fill()

t.pen(pencolor="purple", fillcolor="orange", pensize=1, speed=9)
t.right(180)
t.forward(120)
t.begin_fill()
t.right(60)
t.forward(120)
t.right(120)
t.forward(120)
t.end_fill()

t.pen(pencolor="purple", fillcolor="yellow", pensize=1, speed=9)
t.begin_fill()
t.left(120)
t.forward(120)
t.left(120)
t.forward(120)
t.end_fill()

t.pen(pencolor="purple", fillcolor="purple", pensize=1, speed=9)
t.right(180)
t.forward(120)
t.begin_fill()
t.right(60)
t.forward(120)
t.right(120)
t.forward(120)
t.end_fill()



